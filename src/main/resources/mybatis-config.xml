<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
   <settings>
      <setting name="jdbcTypeForNull" value="NULL"></setting>
      <!-- 만약에 null로 데이터가 전달되었으면 빈칸이 아닌 null로 인식하도록 설정 -->
   </settings>
   <typeAliases>
      <typeAlias type="com.pick.my.community.domain.Community_File" alias="File" />
      <typeAlias type="com.pick.my.community.domain.Community_Post" alias="Post"/>
      <typeAlias type="com.pick.my.supporting.domain.Supporting" alias="Supporting"></typeAlias>
      <typeAlias type="com.pick.my.supporting.domain.SupReply" alias="SupReply"></typeAlias>
      <typeAlias type="com.pick.my.supporting.domain.SupFile" alias="SupFile"></typeAlias>
      <typeAlias type="com.pick.my.goods.domain.Goods" alias="Goods"/>
      <typeAlias type="com.pick.my.goods.domain.GoodsFile" alias="GoodsFile"/>
      <typeAlias type="com.pick.my.goods.domain.GoodsPayment" alias="GoodsPayment"/>
      <typeAlias type="com.pick.my.goods.domain.Review" alias="Review"/>
      <typeAlias type="com.pick.my.idol.domain.Idol" alias="Idol"/>
   </typeAliases>
   <mappers>
      <mapper resource="mappers/supporting-mapper.xml"></mapper>
      <mapper resource="mappers/community-mapper.xml"></mapper>
      <mapper resource="mappers/goods-mapper.xml"></mapper>
      <mapper resource="mappers/idol-mapper.xml"/>
   </mappers>
</configuration>